'mutables
list = [ 1, "hello!", 3.5, False, 1]
dict = { a:1, b:"hello!", c:3.5, d:False }
set_from_list = set(list)

'immutables
ints = 1
strings = "hello!"
tuples = ( 1, "hello!", 3.5, False, 1, 2)
bools = True

set_from_tuple = set(tuples)

sorted_variables = {
    mutables = { list, dict, set_from_list, set_from_tuple}
    immutables = { ints, strings, tuples, bools}
}
